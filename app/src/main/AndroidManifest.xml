<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myself_view"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:name="com.example.myselfview.index.MoocApplication"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.myselfview.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.myselfview.activity.DialogActivity"
            android:label="对话框效果"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.ScoreActivity"
            android:label="分数效果"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.PicActivity"
            android:label="照片选取效果"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.RelativeLayoutTestActivity"
            android:label="自己写的相对布局测试"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.PicFullScreenActivity"
            android:label="图片全屏实例"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.PicFullScreenShowActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.CircleImgShowActivity"
            android:label="圆角图片显示"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.CircleImgLocalActivity"
            android:label="本地图片"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.CircleImgNetActivity"
            android:label="网络图片"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.activity.ScrollListPagerActivity"
            android:label="ScrollView+ViewPager+ListView"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.sections.jdtoast.ToastTestActivity"
            android:label="防制京东的带图标的toast"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.sections.wave.WaveActivity"
            android:label="支付宝声波效果"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.sections.gridview.GridviewDemoActivity"
            android:label="gridview+header view"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.sections.cookie.HttpCookieTestActivity"
            android:label="http携带cookie"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.sections.bind.BindServiceActivity"
            android:label="service绑定的2种方式"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.myselfview.sections.camer.CamerServiceActivity"
            android:label="camera在servcie中抓取图片"
            android:screenOrientation="portrait" >
        </activity>

        <service
            android:name="com.example.myselfview.sections.bind.CameraService"
            android:enabled="true" >
        </service>
        <service
            android:name="com.example.myselfview.sections.camer.CameraService"
            android:enabled="true" >
        </service>
    </application>

</manifest>